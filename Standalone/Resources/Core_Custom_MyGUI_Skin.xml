      <State name="highlighted_checked" offsetNamed="Anomalous_Solid_Coord" colour="#{Anomalous_TaskMenuButton_Center_HighlightedChecked}"/>
      <State name="pushed_checked" offsetNamed="Anomalous_Solid_Coord" colour="#{Anomalous_TaskMenuButton_Center_PushedChecked}"/>
    </BasisSkin>
    <!-- Border -->
    <Border width="1">
      <State name="disabled" offsetNamed="Anomalous_Invisible_Coord"/>
      <State name="normal" offsetNamed="Anomalous_Invisible_Coord"/>
      <State name="highlighted" offsetNamed="Anomalous_Solid_Coord" colour="#{Anomalous_TaskMenuButton_Border_Highlighted}"/>
      <State name="pushed" offsetNamed="Anomalous_Solid_Coord" colour="#{Anomalous_TaskMenuButton_Border_Pushed}"/>
