      <State name="highlighted" colour="#{Anomalous_TaskMenuButton_Text_Highlighted}" shift = "false"/>
      <State name="pushed" colour="#{Anomalous_TaskMenuButton_Text_Pushed}" shift = "false"/>
      <State name="disabled_checked" colour="#{Anomalous_TaskMenuButton_Text_DisabledChecked}" shift = "false"/>
      <State name="normal_checked" colour="#{Anomalous_TaskMenuButton_Text_NormalChecked}" shift = "false"/>
